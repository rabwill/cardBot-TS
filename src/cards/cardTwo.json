{
    "type": "AdaptiveCard",   
    "refresh": {
      "userIds": "${userIds}",
      "action": {
        "type": "Action.Execute",
        "verb": "refresh",
        "title": "Refresh",
        "data": {
          "cardId": "${cardId}"
        }
      }
    },
    "body": [
      {
        "type": "TextBlock",
        "text": "Thank you for your vote for the question ' ${question} '",
        "size": "Large",
        "color": "Accent"
      }
  
    ],
    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
    "version": "1.4"
  }